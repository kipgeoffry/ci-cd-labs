pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        dockerContainer { image 'kipgeoffry/jenkins-agent-python:3.9' }
      }
      steps {
        sh 'python3 --version'
      }
    }
    stage('Front-end') {
      agent {
        dockerContainer { image 'kipgeoffry/jenkins-agent-node:20.5.1' }
      }
      steps {
        sh 'node --version'
      }
    }
  }
}